type storage = int;

type parameter =
| ["Add", int]
| ["Sub", int];

type return_ = [list <operation>, storage];

let main = ([action, store]: [parameter, storage]) : return_ => {
  let store2 = store + match (action, {
    Add: (n: int) => n,
    Sub: (n: int) => -n
  });  
  return [(list([]) as list <operation>), store2];
};
