type storage = int;

type parameter =
| ["Increment", int]
| ["Decrement", int];

let add = (a: int, b: int): int => a + b;
let sub = (a: int, b: int): int => a - b;

let main = ([action, store] : [parameter, storage]): [list <operation>, storage] => {
  let store2 = match(action, {
    Increment: (value: int) => add (store, value),
    Decrement: (value: int) => sub (store, value)
  });
  return [(list([]) as list <operation>), store2];
};

